{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [
    "**/.env.*local",
    "tsconfig.json"
  ],
  "pipeline": {
    "build": {
      "cache": true,
      "dependsOn": [],
      "outputs": [
        "dist/**",
        "storybook-static/**"
      ]
    },
    "clean": {
      "cache": false,
      "dependsOn": [],
      "outputs": []
    },
    "clean:all": {
      "cache": false,
      "dependsOn": [],
      "outputs": []
    },
    "dev": {
      "cache": false,
      "dependsOn": [],
      "persistent": true
    },
    "lint": {
      "cache": true,
      "outputs": [],
      "dependsOn": []
    },
    "reset:all": {
      "cache": false,
      "dependsOn": [
        "clean"
      ],
      "outputs": []
    },
    "semantic-release": {
      "cache": false,
      "dependsOn": [
        "build"
      ],
      "outputMode": "new-only"
    },
    "start": {
      "cache": true,
      "dependsOn": [
        "build"
      ],
      "outputs": []
    },
    "typecheck": {
      "cache": true,
      "outputs": [
        "tsconfig.tsbuildinfo"
      ]
    },
    "update": {
      "cache": true,
      "outputs": []
    },
    "update:all": {
      "cache": true,
      "outputs": []
    }
  }
}
