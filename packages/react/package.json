{
  "name": "@nextime-ui/react",
  "version": "2.4.0",
  "description": "NeXTIME UI é uma biblioteca de componentes para React, fundamentada no Chakra-UI. Ela faz a  inclusão do recurso 'use client' em seus componentes, uma estratégia eficaz para solucionar problemas existentes no Next.js versão 13.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts --external react",
    "clean": "rm -rf .turbo dist node_modules pnpm-lock.yaml",
    "dev": "tsup src/index.tsx --format esm,cjs --dts --external react --watch",
    "lint": "eslint src/**/*.ts* --fix",
    "updateAll": "pnpm update --latest"
  },
  "keywords": [
    "React",
    "Chakra-UI",
    "Componentes",
    "UI Library",
    "Next.js",
    "Biblioteca de componentes",
    "NeXTIME UI",
    "use client",
    "Front-end",
    "Desenvolvimento Web",
    "tsup",
    "TypeScript"
  ],
  "author": "Pedro Duarte - NeXTIME",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@nextime-ui/eslint-config": "^2.4.0",
    "@nextime-ui/tokens": "^2.4.0",
    "@nextime-ui/ts-config": "^2.4.0",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7",
    "react": "^18.2.0",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "framer-motion": "^10.15.0"
  }
}
