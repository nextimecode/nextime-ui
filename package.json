{
  "name": "nextime-ui",
  "version": "0.0.0-development",
  "packageManager": "pnpm@8.6.10",
  "engines": {
    "node": "18.x"
  },
  "description": "",
  "private": false,
  "keywords": [],
  "author": "Pedro Duarte - NeXTIME",
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build --parallel",
    "changeset": "changeset",
    "cleanAll": "turbo run clean --parallel && pnpm store prune && rm -rf .next .turbo node_modules tsconfig.tsbuildinfo pnpm-lock.yaml",
    "dev": "turbo run dev --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint --parallel",
    "release": "turbo run build --filter=!docs && changeset publish",
    "semantic-release": "semantic-release",
    "resetAll": "turbo run cleanAll && pnpm i && turbo run build --parallel",
    "typecheck": "turbo run typecheck --parallel",
    "updateAll": "turbo run update --parallel && pnpm update --latest",
    "version-packages": "changeset version"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@swc/core": "^1.3.74",
    "@types/prop-types": "^15.7.5",
    "semantic-release": "^21.0.7",
    "turbo": "^1.10.12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nextimecode/nextime-ui.git"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}
