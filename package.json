{
  "name": "nextime-ui",
  "version": "0.0.0-development",
  "packageManager": "pnpm@8.6.10",
  "engines": {
    "node": "18.x"
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "clean": "turbo run clean --parallel && pnpm store prune && rm -rf node_modules pnpm-lock.yaml",
    "lint": "turbo run lint",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo run build --filter=!docs && changeset publish",
    "typecheck": "turbo run typecheck",
    "updateAll": "turbo run updateAll && pnpm update --latest",
    "semantic-release": "semantic-release"
  },
  "author": "Pedro Duarte - NeXTIME",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@swc/core": "^1.3.74",
    "turbo": "^1.10.12",
    "semantic-release": "^21.0.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nextimecode/nextime-ui.git"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}
